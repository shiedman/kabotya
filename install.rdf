<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>kabotya@shiedman.addons.mozilla.org</em:id>
    <em:version>0.6.5</em:version>
    <em:type>2</em:type>
    <em:unpack>true</em:unpack>
    <em:optionsURL>chrome://kabotya/content/options.xul</em:optionsURL>

    <!-- Target Application this extension can install into,
         with minimum and maximum supported versions. -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.5</em:minVersion>
        <em:maxVersion>19.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>kabotya</em:name>
    <em:description>proxy(gae and dotcloud)/google link remover</em:description>
    <em:creator>shiedman</em:creator>
    <em:homepageURL></em:homepageURL>
    <em:iconURL>chrome://kabotya/skin/images/kabotya-icon.png</em:iconURL>
  </Description>
</RDF>
